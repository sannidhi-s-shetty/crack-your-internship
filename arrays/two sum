https://leetcode.com/problems/two-sum/

# brute force approach
class Solution {
public:
    vector<int> twoSum(vector<int>& nums, int target) {
        vector<int> v;
        for(int i=0;i<nums.size();i++)
        {
            int num1=nums[i];
            for(int j=i+1;j<nums.size();j++)
            {
               if(num1+nums[j]==target && i!=j){
                  v.push_back(i);
                  v.push_back(j);
               } 
                
            }
        }
    return v;}
};
